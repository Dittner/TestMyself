        public function get color():uint {return fillSolidColor.color;}
        public function set color(value:uint):void {fillSolidColor.color = value;}
        ]]></fx:Script>
    <s:fill>
        <s:SolidColor id="fillSolidColor"/>
    </s:fill>
</s:Rect>
